{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:56928",
      "sslPort": 44361
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "ui/graphiql",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "GraphQlProject": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "ui/graphiql",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "StorageConfiguration:Server": "localhost",
        "StorageConfiguration:Database": "graphqldb",
        "StorageConfiguration:UserName": "postgres",
        "StorageConfiguration:Password": "Admin123"
      },
      "dotnetRunMessages": "true",
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/ui/graphiql",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}